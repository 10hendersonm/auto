{
  "name": "auto",
  "bin": "dist/bin/auto.js",
  "description": "CLI tools to help facilitate semantic versioning based on GitHub PR labels",
  "version": "7.0.2",
  "author": {
    "name": "Andrew Lisowski",
    "email": "lisowski54@gmail.com"
  },
  "pkg": {
    "scripts": "../../plugins/**/dist/**/*.js"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/intuit/auto"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "automation",
    "semantic",
    "release",
    "github",
    "labels",
    "automated",
    "continuos integration",
    "changelog",
    "cli"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:watch": "npm run build -- -w",
    "lint": "tslint -p . --format stylish",
    "test": "jest --maxWorkers=2",
    "bundle": "rimraf binary && pkg . --out-path binary && yarn gzip",
    "gzip": "ls binary/auto* | xargs gzip"
  },
  "dependencies": {
    "@auto-it/core": "^7.0.2",
    "@auto-it/npm": "^7.0.2",
    "@auto-it/released": "^7.0.2",
    "chalk": "^2.4.2",
    "command-line-args": "^5.1.1",
    "command-line-usage": "^5.0.5",
    "dedent": "^0.7.0",
    "signale": "^1.4.0"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/command-line-usage": "^5.0.1",
    "@types/dedent": "^0.7.0",
    "@types/signale": "^1.2.1",
    "pkg": "^4.4.0",
    "rimraf": "^2.6.3"
  }
}
